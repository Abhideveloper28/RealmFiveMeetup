<div class="page-header"><h1>Import Group</h1></div>
<div class="sub-container">
  <div class="form-group">
    <label for="exampleFormControlFile1">Import data to CSV</label>
    <%= form_tag groups_import_path, multipart: true do %>
      <%= file_field_tag :file, class: "form-control-file" %>
      <div class='row mt-20'>
        <div class='col-md-6'>
          <%= submit_tag 'Import CSV', class: 'btn btn-info btn-width' %>
        </div>
      </div>
    <% end %>
  </div>
</div>
